{
	"name": "dataflow11",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Json1",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "flatten1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          store as (name as string, location as string, categories as (category_id as string, name as string, products as (product_id as string, name as string, price as double, supplier as (supplier_id as string, name as string, contact as (phone as string, email as string)), specs as (processor as string, ram as string, storage as string, battery_life as string))[])[])",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     documentForm: 'singleDocument') ~> source1",
				"source1 foldDown(unrollMultiple(store.categories,store.categories.products),",
				"     mapColumn(",
				"          cateogory_id = store.categories.category_id,",
				"          category_name = store.categories.name,",
				"          product_id = store.categories.products.product_id,",
				"          product_name = store.categories.products.name,",
				"          supplier_id = store.categories.products.supplier.supplier_id,",
				"          supplier_name = store.categories.products.supplier.name,",
				"          phone = store.categories.products.supplier.contact.phone,",
				"          email = store.categories.products.supplier.contact.email,",
				"          processor = store.categories.products.specs.processor,",
				"          ram = store.categories.products.specs.ram,",
				"          storage = store.categories.products.specs.storage,",
				"          battery_life = store.categories.products.specs.battery_life",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> flatten1"
			]
		}
	}
}